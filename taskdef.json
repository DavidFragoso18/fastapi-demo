{
  "family": "fastapi-demo-td",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "web",
      "image": "924609080129.dkr.ecr.eu-west-1.amazonaws.com/fastapi-demo:latest",
      "portMappings": [{ "containerPort": 80, "protocol": "tcp" }],
      "essential": true
    }
  ]
}
